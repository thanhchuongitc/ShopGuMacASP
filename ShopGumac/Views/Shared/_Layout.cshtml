        function loadSmartSearch() {

            $.ajax({
                url: "/SmartSearch/Index",
                method: "POST",
                data: { key: $("#txtSearch").val() },
                success: function (data) {
                    $("#search_recommend_content").html(data);
                    hideSpinSearchLoading();
                }
            });

        }
        $('#txtSearch').keypress(function (event) {

            var keycode = (event.keyCode ? event.keyCode : event.which);
            if (keycode == '13') {
                var key = $("#txtSearch").val();
                if (key != '') {
                    $.ajax({
                        url: "/Wproducts/LinkSearch",
                        method: "POST",
                        data: { LinkSearch: key },
                        success: function (data) {

                            var link = data.result.replace(/ /g, "-");
                            window.location.href = '/' + link + '.html';

                        }
                    });
                }
            }

        });
        function SearchTop() {

            var key = $("#txtSearch").val();
            $.ajax({
                url: "/Wproducts/LinkSearch",
                method: "POST",
                data: { LinkSearch: key },
                success: function (data) {

                    var link = data.result.replace(/ /g, "-");
                    window.location.href = '/' + link + '.html';

                }
            });

        }
        $(document).click(function (e) {

            var container = $(".search_content");
            var content = $("#search_recommend")
            if (!container.is(e.target) && container.has(e.target).length === 0) {
                content.hide();
            }
        });

        function onSearchDelay(id, fn) {
            var keytime = new Date();
            $(id).keyup(function () {
                showSpinSearchLoading();
                keytime = new Date();
                setTimeout(function () {
                    var current = new Date();
                    var dif = current - keytime;
                    if (dif > 500) {
                        fn(id);
                    }
                }, 550);
            });
        }

        function reloadSearch(key) {
            $("#txtSearch").val(key);
            loadSmartSearch();
        }

        function getCartHeader() {
            $.ajax({
                url: "/Cart/TopCartView",
                method: "POST",
                data: { isHeader: "true" },
                success: function (data) {

                    $("#info-icon-cart").html(data);
                }
            });
        }
    </script>


