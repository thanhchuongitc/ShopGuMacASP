            function addCart(productID, quantity, callback, note) {

                $.ajax({
                    type: "POST",
                    url: '/cart/addcart',
                    data: { productID: productID, quantity: quantity, note: note },
                    beforeSend: function () {
                        showSpinLoadingShop();
                    },
                    success: function (data) {

                        if (data.Code === 1) {
                            getCartHeader();
                            $('.numberCart').html(data.TotalItem);
                            if (callback == true) {
                                window.location.reload();
                            }
                            else {
                                window.location.href = "cart.html";
                            }
                        } else {
                            hideSpinLoadingShop();
                            showError('Lỗi', data.message);
                        }
                    }
                });
            }
            function addSetToCart(setId, quantity, arrayProducts, callback, note) {
                $.ajax({
                    type: "POST",
                    url: '/cart/addsettocart',
                    data: {
                        setId: setId,
                        quantity: quantity,
                        listproduct: arrayProducts,
                        note: note
                    },
                    beforeSend: function () {
                        //ematics("log", "product", "cart", products);
                        showSpinLoadingShop();
                    },
                    success: function (data) {
