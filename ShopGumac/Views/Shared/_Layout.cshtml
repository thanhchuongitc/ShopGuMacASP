        <script>
            $(document).ready(function () {

            });

            function changeGender() {
                if ($("input#female").is(':checked'))
                    $("#CustomerGender").val("Nu");
                else
                    $("#CustomerGender").val("Nam");
            }

            function changeBirthday() {
                var day = $("#day").val();
                var month = $("#month").val();
                var year = $("#year").val();
                if (day != '' && month != '' && year != '')
                    $("#Birthday").val(year + "/" + month + "/" + day);
            }

            $('#promotion').change(function () {
                $("#IsAgressEmail").val($("input#promotion").is(':checked'));
            });

            $("#w_pass").on('keypress', function (e) {
                //Enter key
                if (e.which === 13) {
                    wlogin();
                }
            });


            $("#w_email").on('keypress', function (e) {
                //Enter key
                if (e.which === 13) {
                    wlogin();
                }
            });

            function wlogin() {
                var email = $('#w_email').val();
                var pass = $('#w_pass').val();
                $.ajax({
                    url: '@Url.Action("Login","User")',
                    type: "POST",
                    data: {
                        email: email, password: pass
                    },
                    beforeSend: function () {
                        showSpinLoading();
                    },
                    success: function (data) {
                        debugger
                        if (data=="1") {
                            $('#modalLogin').modal("hide");
                            $.notify("Thành công","success");
                            setTimeout(() => {
                                hideSpinLoading();
                                location.reload();},500)
                        }
                        else {
                            $.notify("Thất bại");
                            setTimeout(() => { hideSpinLoading();});      
                        }
                    },
                    error: function (xhr, status, error) {
                        debugger
                        
                    }

                });
            }

            function showGetPassCustomer() {
                $.ajax({
                    url: "/WLogin/GetPasswordCustomer",
                    method: "POST",
                    beforeSend: function () {
                        showSpinLoading();
                    },
                    success: function (data) {
                        hideSpinLoading();
                        $('#modalLogin').modal("hide");
                        $("#popupContainer2").html(data);
                        $("#ModalGetPass").modal("show");

                    }
                });
            }

            function saveRegister() {
                var validation = validate();
                if (validation == true) {
                    $.ajax({
                        url: '@Url.Action("Register","User")',
                        type: "POST",
                        data: {
                            name: $('#w_reg_fullname').val(),
                            phone: $('#w_reg_phone').val(),
                            address: $('#w_reg_add').val(),
                            email: $('#w_reg_email').val(),
                            password: $('#w_reg_password').val(),
                            sex: $('input[name="CustomerGender"]').val(),
                            birthday: $('#Birthday').val()
                        },
                        beforeSend: function () {
                            showSpinLoading();
                        },
                        success: function (data) {
                            hideSpinLoading();
                            $('#tabRegister').removeClass("active");
                            $('#createAccount').removeClass("active in");
                            $('#tabLogin').addClass("active");
                            $('#login').addClass("active in");
                            $('#formRegister').trigger("reset");
                            location.reload();
                    },
                        error: function () {
                            hideSpinLoading();
                            showError('Lỗi', data.message);
                        }
                    });
                }
            }

            function validate() {
                if ($('#frmRegister input[name="CustomerName"]').val() == "") {
                    showError("Lỗi", "Bạn chưa nhập họ tên");
                    return false;
                }
                if ($('#frmRegister input[name="CustomerEmail"]').val() == "") {
                    showError("Lỗi", "Bạn chưa nhập Email");
                    return false;
                }
                if ($('#frmRegister input[name="CustomerPhone1"]').val() == "") {
                    showError("Lỗi", "Bạn chưa nhập số điện thoại");
                    return false;
                }
                var regex = /^[0-9]+$/;
                if (!regex.test($('#frmRegister input[name="CustomerPhone1"]').val()) || $('#frmRegister input[name="CustomerPhone1"]').val().length < 10) {
                    showError("Lỗi", "Số điện thoại không đúng định dạng");
                    return false;
                }

                if ($('#frmRegister input[name="CustomerGender"]').val() == "") {
                    showError("Lỗi", "Bạn chưa chọn giới tính");
                    return false;
                }
                if ($('#frmRegister input[name="Password"]').val() == "") {
                    showError("Lỗi", "Bạn chưa nhập mật khẩu");
                    return false;
                }
                if ($('#frmRegister input[name="Birthday"]').val() == "") {
                    showError("Lỗi", "Bạn chưa chọn ngày sinh");
                    return false;
                }
                return true;
            }
        </script>
