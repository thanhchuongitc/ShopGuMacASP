    <script>

        $(document).ready(function () {
            $(".menu-opener").click(function () {
                $(".menu-opener, .menu-opener-inner, .menu").toggleClass("active");
                if ($(this).is(".active")) {
                    $(".shop-home").css({ 'overflow-y': 'hidden' });
                    $(".offcanvas").addClass('is-active');
                    $(".global-overlay").css({ 'display': 'block' });
                }
                else {
                    $(".shop-home").css({ 'overflow-y': 'auto' });
                    $(".offcanvas").removeClass('is-active');
                    $(".global-overlay").css({ 'display': 'none' });
                }
            });
            //$(".active").click(function () {
            //    debugger;
            //    $(".shop-home").css({ 'overflow-y': 'auto' });
            //    $(".offcanvas").removeClass('is-active');
            //    $(".global-overlay").css({ 'display': 'none' });
            //});
            $(".dropdown").click(function () {
                $(this).toggleClass('open');
                $(this).toggle('aria-expanded', "true");

            });
            $(".user_header").click(function () {
                $("#lg_header_content").animate({
                    height: 'toggle'
                });
                $("#lg_header_cart").hide();
            });

            $(".user_cart").click(function () {
                $("#lg_header_cart").animate({
                    height: 'toggle'
                });
                $("#lg_header_content").hide();
            });

            setNavigation();

            $("#txtSearch").click(function () {
                $("#search_recommend").css({ "display": "block" });
            });
        });

        function setNavigation() {
            $(".nav a").each(function () {
                if (this.href == window.location.href) {
                    $(this).closest('li a').addClass('active');
                }
            });
        }


    </script>
