            $("nav.offcanvas").css({ "min-height": wheight, "overflow-y": "scroll" });

            var menuh = wheight - $("#menusp_header").height() - $("#menusp_footer").height();

            $("ul#menutreeSP").css({ "height": menuh, "overflow-y": "scroll" });

            onSearchDelay("#txtSearch", function () {
